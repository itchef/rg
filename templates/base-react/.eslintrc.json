{
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "shallow": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": [
      "error", {"devDependencies": ["./src/setupTests.js", "**/*.test.js"]}
    ]
  }
}
